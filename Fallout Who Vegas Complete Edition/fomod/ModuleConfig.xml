<!-- Created with FOMOD Creation Tool 1.7.0.37 [http://www.nexusmods.com/fallout4/mods/6821] --> 
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://qconsulting.ca/fo3/ModConfig5.0.xsd"> 
	<moduleName>Fallout Who Vegas - Complete Edition</moduleName> 
	<moduleImage path="favicon-96x96.png" /> 
	<installSteps order="Explicit"> 
		<installStep name="The Foundry"> 
			<optionalFileGroups order="Explicit"> 
				<group name="Fallout Who Vegas" type="SelectAny"> 
					<plugins order="Explicit"> 
						<plugin name="Base Mod"> 
							<description>Fallout Who Vegas! This is the core mod that must be installed for anything else listed here to work.</description> 
							<files> 
								<folder source="WhoVegas" destination="" priority="0" /> 
							</files> 
							<typeDescriptor> 
								<type name="Required"/> 
							</typeDescriptor> 
						</plugin> 
						<plugin name="51st Century Communicator"> 
							<description>An optional addon for Fallout Who Vegas which changes your pipboy into River Song's communicator from "The Pandorica Opens"</description> 
							<files> 
								<folder source="Communicator" destination="" priority="0" /> 
							</files> 
							<typeDescriptor> 
								<type name="Optional"/> 
							</typeDescriptor> 
						</plugin> 
					</plugins> 
				</group> 
				<group name="Exterior Improvements" type="SelectAny"> 
					<plugins order="Explicit"> 
						<plugin name="Improved Models"> 
							<description>Adds improved models for the 2010 and 2014 TARDIS exteriors</description> 
							<files> 
								<folder source="JackDependency\meshes" destination="meshes" priority="0" /> 
								<folder source="JackDependency\textures" destination="textures" priority="0" /> 
							</files> 
							<typeDescriptor> 
								<type name="Required"/> 
							</typeDescriptor> 
						</plugin> 
					</plugins> 
				</group> 
			</optionalFileGroups> 
		</installStep> 
		<installStep name="Xoanon"> 
			<optionalFileGroups order="Explicit"> 
				<group name="Essential" type="SelectAny"> 
					<plugins order="Explicit"> 
						<plugin name="Essentials"> 
							<description>Xoanon's Fixes for Fallout Who Vegas</description> 
							<files> 
								<folder source="Xoanon\LODSettings" destination="LODSettings" priority="0" /> 
								<folder source="Xoanon\meshes" destination="meshes" priority="0" /> 
								<folder source="Xoanon\sound" destination="sound" priority="0" /> 
								<folder source="Xoanon\textures" destination="textures" priority="0" /> 
								<file source="Xoanon\fwv.esm" destination="fwv.esm" priority="0" /> 
								<file source="Xoanon\fwvXoanon.esm" destination="fwvXoanon.esm" priority="0" /> 
								<file source="Xoanon\fwvXoanon.esp" destination="fwvXoanon.esp" priority="0" /> 
							</files> 
							<typeDescriptor> 
								<type name="Required"/> 
							</typeDescriptor> 
						</plugin> 
					</plugins> 
				</group> 
				<group name="Optional Gameplay" type="SelectAny"> 
					<plugins order="Explicit"> 
						<plugin name="Personal Preference Extras"> 
							<description>Extra 'set dressing' in TARDIS, extra Refining items, Garden seeds respawn, Water instead of Sunset Sarsaparilla in the Drinks Machine, easier Piloting XP grind.</description> 
							<files> 
								<file source="Xoanon\fwvXoanon_PersPref.esp" destination="fwvXoanon_PersPref.esp" priority="0" /> 
							</files> 
							<typeDescriptor> 
								<type name="Recommended"/> 
							</typeDescriptor> 
						</plugin> 
						<plugin name="Quest Sounds"> 
							<description>Minor and optional change to Who-ify the Quest Popup, Quest Complete, Map Location, and Wild Wasteland sounds.</description> 
							<files> 
								<file source="Xoanon\fwvXoanon_QuestSounds.esp" destination="fwvXoanon_QuestSounds.esp" priority="0" /> 
							</files> 
							<typeDescriptor> 
								<type name="Optional"/> 
							</typeDescriptor> 
						</plugin> 
						<plugin name="Laser Screwdriver Edits"> 
							<description>Changes Laser Screwdriver beam animation, removes disintegration, changes sound. Karma penalty for using 'cruel' Laser Screwdriver modes.</description> 
							<files> 
								<file source="Xoanon\fwvXoanonLaserScrewdriverEdits.esp" destination="fwvXoanonLaserScrewdriverEdits.esp" priority="0" /> 
							</files> 
							<typeDescriptor> 
								<type name="Recommended"/> 
							</typeDescriptor> 
						</plugin> 
						<plugin name="No SPECIAL Randomise on Regeneration"> 
							<description>When a Timelord player character regenerates, their SPECIAL points will not be randomized.</description> 
							<files> 
								<file source="Xoanon\fwvXoanonNoSPECIALRandomizeOnRegen.esp" destination="fwvXoanonNoSPECIALRandomizeOnRegen.esp" priority="0" /> 
							</files> 
							<typeDescriptor> 
								<type name="Optional"/> 
							</typeDescriptor> 
						</plugin> 
						<plugin name="Revised Companions"> 
							<description>Improvements to Companion code. Overhauls Cyber companion to Classic Style with new model, weapons, dialogue. Highly advised. Left optional for players wanting 'modern' Cyberman instead.</description> 
							<files> 
								<file source="Xoanon\fwvXoanonRevisedCompanions.esp" destination="fwvXoanonRevisedCompanions.esp" priority="0" /> 
							</files> 
							<typeDescriptor> 
								<type name="Recommended"/> 
							</typeDescriptor> 
						</plugin> 
						<plugin name="Outfit Bonuses"> 
							<description>Different outfits specific to DW characters give slight bonuses or penalties to match their 'owners'. One might have higher science but lower charisma, one might have higher charisma but lower Guns skill...</description> 
							<files> 
								<file source="Xoanon\fwvXoanonOutfitBonuses.esp" destination="fwvXoanonOutfitBonuses.esp" priority="0" /> 
							</files> 
							<typeDescriptor> 
								<type name="Recommended"/> 
							</typeDescriptor> 
						</plugin> 
					</plugins> 
				</group> 
				<group name="Gamepad Users" type="SelectAny"> 
					<plugins order="Explicit"> 
						<plugin name="Gamepad Players Protocols Help"> 
							<description>Install this if you use a Gamepad/Controller to play Who Vegas!</description> 
							<files> 
								<file source="Xoanon\fwvXoanonGamepadProtocols.esp" destination="fwvXoanonGamepadProtocols.esp" priority="0" /> 
							</files> 
							<typeDescriptor> 
								<type name="Optional"/> 
							</typeDescriptor> 
						</plugin> 
						<plugin name="Gamepad Players Weapon Menu"> 
							<description>Install this if you use a Gamepad/Controller to play Who Vegas!</description> 
							<files> 
								<file source="Xoanon\fwvXoanonGamepadWeaponMenu.esp" destination="fwvXoanonGamepadWeaponMenu.esp" priority="0" /> 
							</files> 
							<typeDescriptor> 
								<type name="Optional"/> 
							</typeDescriptor> 
						</plugin> 
					</plugins> 
				</group> 
				<group name="ENB Users" type="SelectAny"> 
					<plugins order="Explicit"> 
						<plugin name="TARDIS ENB Lighting Fix"> 
							<description>For ENB users. Select to prevent a graphic glitch in the TARDIS when running an ENB shader. Not needed if you don't use ENB, but shouldn't cause problems.</description> 
							<files> 
								<file source="Xoanon\fwvXoanonENBFix03.esp" destination="fwvXoanonENBFix03.esp" priority="0" /> 
							</files> 
							<typeDescriptor> 
								<type name="Optional"/> 
							</typeDescriptor> 
						</plugin> 
					</plugins> 
				</group> 
			</optionalFileGroups> 
		</installStep> 
		<installStep name="Spyduck"> 
			<optionalFileGroups order="Explicit"> 
				<group name="Console Room Addon" type="SelectAny"> 
					<plugins order="Explicit"> 
						<plugin name="Console Room Additions"> 
							<description>Adds improvements to all console rooms plus adds the Secondary console room from Classic Doctor Who.</description> 
							<files> 
								<file source="Spyduck\Console\fwvSpyduckThemeUpdates.esm" destination="fwvSpyduckThemeUpdates.esm" priority="0" /> 
								<file source="Spyduck\Console\fwvSpyduckThemeUpdates.esp" destination="fwvSpyduckThemeUpdates.esp" priority="0" /> 
								<folder source="Spyduck\Console\meshes" destination="meshes" priority="0" /> 
								<folder source="Spyduck\Console\sound" destination="sound" priority="0" /> 
								<folder source="Spyduck\Console\textures" destination="textures" priority="0" /> 
							</files> 
							<typeDescriptor> 
								<type name="Recommended"/> 
							</typeDescriptor> 
						</plugin> 
					</plugins> 
				</group> 
				<group name="Regeneration Deuglifier" type="SelectAny"> 
					<plugins order="Explicit"> 
						<plugin name="Regeneration Deuglifier"> 
							<description>Makes regenerations look more like actual human beings, fixes some issues with random face generation for Regeneration.</description> 
							<files> 
								<file source="Spyduck\Deuglifier\fwv Patch Regeneration Deuglifier.esp" destination="fwv Patch Regeneration Deuglifier.esp" priority="0" /> 
								<file source="Xoanon\fwvXoanonDeuglifierCompat.esp" destination="fwvXoanonDeuglifierCompat.esp" priority="0" /> 
							</files> 
							<typeDescriptor> 
								<type name="Recommended"/> 
							</typeDescriptor> 
						</plugin> 
					</plugins> 
				</group> 
			</optionalFileGroups> 
		</installStep> 
		<installStep name="Taylor7500"> 
			<optionalFileGroups order="Explicit"> 
				<group name="Tweaks" type="SelectAny"> 
					<plugins order="Explicit"> 
						<plugin name="Advanced Mode Recall"> 
							<description>Allows the player to summon the TARDIS using their Sonic Screwdriver in Advanced Mode as well as Basic Mode</description> 
							<image path="Taylor\Mod Images\60613-1603450009-848304399 (1).png" /> 
							<files> 
								<file source="Taylor\FwvTweakAdvancedModeRecall.esp" destination="FwvTweakAdvancedModeRecall.esp" priority="0" /> 
							</files> 
							<typeDescriptor> 
								<type name="Optional"/> 
							</typeDescriptor> 
						</plugin> 
						<plugin name="Infinite Regenerations from Cracks"> 
							<description>Prevents cracks in time from despawning, allowing the player to have infinite regeneration cycles</description> 
							<image path="Taylor\Mod Images\TimeFissure (1).png" /> 
							<files> 
								<file source="Taylor\FwvTweakTimeCrackNoDisable.esp" destination="FwvTweakTimeCrackNoDisable.esp" priority="0" /> 
							</files> 
							<typeDescriptor> 
								<type name="Optional"/> 
							</typeDescriptor> 
						</plugin> 
						<plugin name="Sonic Strength Requirements"> 
							<description>Sets the STR requirement for all Sonic Screwdriver models to 1. Intended for use with other mods which factor weapon STR into things.</description> 
							<image path="Taylor\Mod Images\Sonic.png" /> 
							<files> 
								<file source="Taylor\FwvTweakSonicStrength.esp" destination="FwvTweakSonicStrength.esp" priority="0" /> 
							</files> 
							<typeDescriptor> 
								<type name="Optional"/> 
							</typeDescriptor> 
						</plugin> 
						<plugin name="Voluntary Regeneration"> 
							<description>Adds the option to regenerate to the telepathy menu, so that it can be triggered anywhere</description> 
							<image path="Taylor\Mod Images\Regen.jpg" /> 
							<files> 
								<file source="Taylor\FwvTweakVoluntaryRegeneration.esp" destination="FwvTweakVoluntaryRegeneration.esp" priority="0" /> 
							</files> 
							<typeDescriptor> 
								<type name="Optional"/> 
							</typeDescriptor> 
						</plugin> 
						<plugin name="Human Medicine Doesn't Poison Time Lords"> 
							<description>Removes the restriction that stimpaks and other human medicine acts as poison to Time Lord players</description> 
							<image path="Taylor\Mod Images\NoPoison.png" /> 
							<files> 
								<file source="Taylor\FwvTweakNoPoisonTimeLord.esp" destination="FwvTweakNoPoisonTimeLord.esp" priority="0" /> 
							</files> 
							<typeDescriptor> 
								<type name="Optional"/> 
							</typeDescriptor> 
						</plugin> 
						<plugin name="Skip Manual Intervention Quest"> 
							<description>Adds an option to skip the "Manual Intervention" quest, intended for experienced players who do not wish to collect all 15 pages again.</description> 
							<image path="Taylor\Mod Images\SkipManualIntervention.png" /> 
							<files> 
								<file source="Taylor\FwvTweakSkipMenialQuests.esp" destination="FwvTweakSkipMenialQuests.esp" priority="0" /> 
							</files> 
							<typeDescriptor> 
								<type name="Optional"/> 
							</typeDescriptor> 
						</plugin> 
						<plugin name="Clearer Room Toggle Text"> 
							<description>Replaces potentially ambiguous text when toggling rooms via the Architectural Reconfiguration system with text which is more clear</description> 
							<image path="Taylor\Mod Images\RoomToggleText.png" /> 
							<files> 
								<file source="Taylor\FwvTweakRoomToggleText.esp" destination="FwvTweakRoomToggleText.esp" priority="0" /> 
							</files> 
							<typeDescriptor> 
								<type name="Optional"/> 
							</typeDescriptor> 
						</plugin> 
					</plugins> 
				</group> 
			</optionalFileGroups> 
		</installStep> 
		<installStep name="Xyphos"> 
			<optionalFileGroups order="Explicit"> 
				<group name="" type="SelectAny"> 
					<plugins order="Explicit"> 
						<plugin name="None"> 
							<description>Select this if you do not have any of the Fallout New Vegas DLCs or Tale of Two Wastelands installed.</description> 
							<typeDescriptor> 
								<type name="Optional"/> 
							</typeDescriptor> 
						</plugin> 
						<plugin name="Tale of Two Wastelands Patch"> 
							<description>Allows the TARDIS to work with the "Tale of Two Wastelands" mod for Fallout New Vegas, letting you use the TARDIS in the Capital Wasteland!</description> 
							<files> 
								<file source="Xyphos\Fallout Who Vegas TTW Addon.esp" destination="Fallout Who Vegas TTW Addon.esp" priority="0" /> 
							</files> 
							<typeDescriptor> 
								<dependencyType> 
									<defaultType name="Optional"/> 
									<patterns> 
										<pattern> 
											<dependencies operator="And"> 
												<fileDependency file="DeadMoney.esm" state="Active"/> 
												<fileDependency file="HonestHearts.esm" state="Active"/> 
												<fileDependency file="OldWorldBlues.esm" state="Active"/> 
												<fileDependency file="LonesomeRoad.esm" state="Active"/> 
												<fileDependency file="Fallout3.esm" state="Active"/> 
											</dependencies> 
											<type name="Recommended"/> 
										</pattern> 
										<pattern> 
											<dependencies operator="Or"> 
												<fileDependency file="DeadMoney.esm" state="Missing"/> 
												<fileDependency file="HonestHearts.esm" state="Missing"/> 
												<fileDependency file="OldWorldBlues.esm" state="Missing"/> 
												<fileDependency file="LonesomeRoad.esm" state="Missing"/> 
												<fileDependency file="Fallout3.esm" state="Missing"/> 
											</dependencies> 
											<type name="NotUsable"/> 
										</pattern> 
									</patterns> 
								</dependencyType> 
							</typeDescriptor> 
						</plugin> 
						<plugin name="DLC Compatibility Patch"> 
							<description>Allows the TARDIS to access base Fallout New Vegas DLC locations.</description> 
							<files> 
								<file source="Xyphos\Fallout Who Vegas NV Dlcs Addon.esp" destination="Fallout Who Vegas NV Dlcs Addon.esp" priority="0" /> 
							</files> 
							<typeDescriptor> 
								<dependencyType> 
									<defaultType name="Optional"/> 
									<patterns> 
										<pattern> 
											<dependencies operator="And"> 
												<fileDependency file="DeadMoney.esm" state="Active"/> 
												<fileDependency file="HonestHearts.esm" state="Active"/> 
												<fileDependency file="OldWorldBlues.esm" state="Active"/> 
												<fileDependency file="LonesomeRoad.esm" state="Active"/> 
											</dependencies> 
											<type name="Recommended"/> 
										</pattern> 
										<pattern> 
											<dependencies operator="Or"> 
												<fileDependency file="DeadMoney.esm" state="Missing"/> 
												<fileDependency file="HonestHearts.esm" state="Missing"/> 
												<fileDependency file="OldWorldBlues.esm" state="Missing"/> 
												<fileDependency file="LonesomeRoad.esm" state="Missing"/> 
											</dependencies> 
											<type name="NotUsable"/> 
										</pattern> 
									</patterns> 
								</dependencyType> 
							</typeDescriptor> 
						</plugin> 
					</plugins> 
				</group> 
			</optionalFileGroups> 
		</installStep> 
		<installStep name="Miscellaneous"> 
			<optionalFileGroups order="Explicit"> 
				<group name="Chr1s0g's Addons" type="SelectAny"> 
					<plugins order="Explicit"> 
						<plugin name="Low-Resolution Quality Texture Patch"> 
							<description>Adds assets to the game which allow items with missing Low-Quality textures to display the correct textures.</description> 
							<files> 
								<folder source="Chr1s0g\textures" destination="textures" priority="0" /> 
							</files> 
							<typeDescriptor> 
								<type name="Optional"/> 
							</typeDescriptor> 
						</plugin> 
						<plugin name="Motion Sickness "> 
							<description>This patch was made for users with motion sickness to disable the TARDIS causing the player's camera to shake while in flight.</description> 
							<files> 
								<file source="Chr1s0g\fwvNoShake.esp" destination="fwvNoShake.esp" priority="0" /> 
							</files> 
							<typeDescriptor> 
								<type name="Optional"/> 
							</typeDescriptor> 
						</plugin> 
					</plugins> 
				</group> 
				<group name="Mackquinn00's Addons" type="SelectAny"> 
