							</files> 
							<typeDescriptor> 
								<type name="Recommended"/> 
							</typeDescriptor> 
						</plugin> 
					</plugins> 
				</group> 
				<group name="Jack's Addons" type="SelectAny"> 
					<plugins order="Explicit"> 
						<plugin name="Improved 2005 Exterior"> 
							<description>An additional improved model for the 2005 (Coral) TARDIS exterior</description> 
							<files> 
								<folder source="Jack\meshes" destination="meshes" priority="0" /> 
								<folder source="Jack\textures" destination="textures" priority="0" /> 
							</files> 
							<typeDescriptor> 
								<type name="Recommended"/> 
							</typeDescriptor> 
						</plugin> 
					</plugins> 
				</group> 
				<group name="Ninfriendo's Addons" type="SelectAny"> 
					<plugins order="Explicit"> 
						<plugin name="Time Lord Arsenal"> 
							<description>Adds the Time Lord Staser weapon to the Tomb of Rassilon.</description> 
							<files> 
								<file source="Ninfriendo\TimeLordArsenal\TimeLordArsenal.esp" destination="TimeLordArsenal.esp" priority="0" /> 
								<folder source="Ninfriendo\TimeLordArsenal\meshes" destination="meshes" priority="0" /> 
								<folder source="Ninfriendo\TimeLordArsenal\textures" destination="textures" priority="0" /> 
							</files> 
							<typeDescriptor> 
								<type name="Optional"/> 
							</typeDescriptor> 
						</plugin> 
					</plugins> 
				</group>
				<group name="coolluke1992's Addons" type="SelectAny"> 
					<plugins order="Explicit"> 
						<plugin name="Fallout Who Tutorial Voices"> 
							<description>Adds Tutorial Voices.</description> 
							<files> 
								<file source="coolluke1992\fwvVoices.esp" destination="fwvVoices.esp" priority="0" /> 
								<folder source="coolluke1992\Sound" destination="Sound" priority="0" /> 
							</files> 
							<typeDescriptor> 
								<type name="Optional"/> 
							</typeDescriptor> 
						</plugin> 
					</plugins> 
				</group>
				<group name="Tock's Addons" type="SelectAny"> 
					<plugins order="Explicit"> 
						<plugin name="Dead Money Sonic Screwdriver"> 
							<description>REQUIRES "Dead Money" DLC Adds a sonic screwdriver to the Sierra Madre for use within the Dead Money DLC.</description> 
							<files> 
								<file source="Tock\fwvDeadMoneySonicScrewdriver\fwvDeadMoneySonic.esp" destination="fwvDeadMoneySonic.esp" priority="0" /> 
								<folder source="Tock\fwvDeadMoneySonicScrewdriver\meshes" destination="meshes" priority="0" /> 
								<folder source="Tock\fwvDeadMoneySonicScrewdriver\textures" destination="textures" priority="0" /> 
							</files> 
							<typeDescriptor> 
								<dependencyType> 
									<defaultType name="Optional"/> 
									<patterns> 
										<pattern> 
											<dependencies operator="And"> 
												<fileDependency file="DeadMoney.esm" state="Active"/> 
											</dependencies> 
											<type name="Optional"/> 
										</pattern> 
										<pattern> 
											<dependencies operator="Or"> 
												<fileDependency file="DeadMoney.esm" state="Missing"/> 
											</dependencies> 
											<type name="NotUsable"/> 
										</pattern> 
									</patterns> 
								</dependencyType> 
							</typeDescriptor> 
						</plugin> 
					</plugins> 
				</group> 
			</optionalFileGroups> 
		</installStep> 
	</installSteps> 
</config>
